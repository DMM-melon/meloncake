<h2>商品新規登録画面</h2>
<div>
  <%= form_for(@product, url: '/admin/products') do |f| %>
  <h2>画像選択</h2>
  <%= f.submit "画像選択" %>

  <h2>商品名</h2>
  <%= f.text_field :name %>

  <h2>商品説明</h2>
  <%= f.text_area :introduction %>

  <h2>ジャンル</h2>

  <h2>税抜き価格</h2>
  <%= f.text_field :price %>

  <h2>販売ステータス</h2>

  <%= f.submit "新規登録" %>
 <% end %>
</div>